php_value memory_limit 128M
RewriteEngine on
ErrorDocument 404 /pryingnet/error.php

RewriteRule ^anasayfa index.php [L,NC]
RewriteRule ^spor spor.php [L,NC]
RewriteRule ^astroloji astroloji.php [L,NC]
RewriteRule ^iletisim iletisim.php [L,NC]
