RewriteRule ^onemli-([0-9a-zA-Z-_]+)-([0-9]+)$ icerik_spslide.php?sef=$1&slider_id=$2 [L,QSA]
RewriteRule ^astro-([0-9a-zA-Z-_]+)-([0-9]+)$ icerik_astro.php?sef=$1&swiper_id=$2 [L,QSA]
RewriteRule ^magazin-([0-9a-zA-Z-_]+)-([0-9]+)$ icerik_magazin.php?sef=$1&slider_id=$2 [L,QSA]
RewriteRule ^schedule-([0-9a-zA-Z-_]+)-([0-9]+)$ icerik_schedule.php?sef=$1&schedule_id=$2 [L,QSA]
RewriteRule ^yenihaberler-([0-9a-zA-Z-_]+)-([0-9]+)$ yenihaberler.php?sef=$1&yenihaber_id=$2 [L,QSA]










